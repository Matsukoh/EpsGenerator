WindowLayout:
    anchor_x: 'center'
    TabbedPanel:
        do_default_tab: False
        tab_width: root.width/2
        TabbedPanelItem:
            text: 'Raster'
            on_release:root._change_mode(self.text)
            CustomSpinner:
                size_hint: None, None
                size: root.width, 50
                # pos_hint: {'center': (0.5, 0.1)}
                text: '1024'
                values: '256', '512', '1024'
                on_text: root._change_export_format(self.text)
        TabbedPanelItem:
            text: 'Vector'
            on_release:root._change_mode(self.text)